                         7     ; Programa: Utilizando rotinas de atraso, escrever um programa em Assembly que gere duas ondas
                         8     ; quadradas: uma com frequência de aproximadamente 10 kHz e outra com frequência de
                         9     ; aproximadamente 20 kHz. Uma solicitação de interrupção externa 0 (Int0 - sensível a
                        10     ; descida de borda) altera entre as duas frequências, enviando o sinal para a saída P2.2 o
                        11     ; que piscará o Led na frequência determinada. O programa deve ficar em loop infinito
                        12     ; alternando as frequências a cada solicitação de interrupção. Inserir uma chave para
                        13     ; acionar a interrupção via Simple Keypad
                        14
                        15     ; ----------------------------------------------------------------------
                        16
                        17                     ORG 0
0000 8003               18                     SJMP    PROG
                        19
                        20                     ORG 0003h
0003 8019               21                     SJMP    EXT_0
                        22
                        23     ; ----------------------------------------------------------------------
                        24
                        25     PROG:           ; setar os pinos de interupçao
0005 D2AF               26                     SETB    EA ; habilita todos os pinos
0007 D2A8               27                     SETB    EX0 ; seta o P3.2
0009 D288               28                     SETB    IT0 ; sensível a borda de descida
                        29
000B C2D5               30                     CLR     F0 ; flag de led ligado,
                        31                                ; F0 = 0: 10 kHZ
                        32                                ; F0 = 1: 20 kHz
                        33
                        34                     ; CONTROLE DE FLUXO!
000D 20D507             35     FLUXO:          JB      F0, FREQ20 ;
                        36
                        37     ; ----------------------------------------------------------------------
                        38
                        39
                        40     FREQ10:         ; Onda quadrada de 10kHZ
                        41
0010 B2A2               42                     CPL     P2.2 ; troca o valor do led
0012 120025             43                     CALL    DELAY10; ; chama o delay
0015 80F6               44                     JMP     FLUXO
                        45
                        46
                        47     FREQ20:         ; Onda quadrada de 20kHZ
                        48
0017 B2A2               49                     CPL     P2.2 ; troca o valor do led
0019 12002B             50                     CALL    DELAY20; ; chama o delay
001C 80EF               51                     JMP     FLUXO
                        52
                        53
                        54     ; ----------------------------------------------------------------------
                        55
001E C2AF               56     EXT_0:          CLR     EA ; desabilita interrupções
                        57
0020 B2D5               58                     CPL     F0 ; troca de delay na subida de borda
                        59
0022 D2AF               60                     SETB    EA; ; reabilita interrupções
0024 32                 61                     RETI
                        62
                        63
                        64     ; ----------------------------------------------------------------------
                        65
                        66     DELAY10:        ; DELAY DE 100 MICROSEGUNDOS (10 kHz)
0025 7831               67                     MOV     R0, #031h
0027 00                 68                     NOP
0028 D8FE               69                     DJNZ    R0, $
002A 22                 70                     RET
                        71
                        72     DELAY20:        ; DELAY DE 50 MICROSEGUNDOS (20 kHz)
002B 7818               73                     MOV     R0, #018h
002D 00                 74                     NOP
002E D8FE               75                     DJNZ    R0, $
0030 22                 76                     RET
                        77
                        78                     END
