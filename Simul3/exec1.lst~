                         7     ; Correções no tempo:
                         8     ; Frquência de 10 kHz: Delay de 50 us no total!
                         9     ; como os processos extras estvam tomando 9 us, coloquei 41 us na special
                        10     ; calculador e a contagem fechou em 51, para "MOV TL0, #0D7h"
                        11
                        12     ; Frquência de 20 kHz: Delay de 25 us no total!
                        13     ; como os processos extras estvam tomando 10 us, coloquei 15 us na special
                        14     ; calculador e a contagem fechou em 25 certinho, para "MOV TL0, #0F1h"
                        15     ; ----------------------------------------------------------------------
                        16
                        17                     ORG     0
0000 801C               18                     SJMP    PROG
                        19     ; ----------------------------------------------------------------------
                        20                     ORG     0003h ; interrupção do INT0
0003 B200               21                     CPL     20h.0 ; complementa o bit de flag 20h.0
                        22                                   ; troca de 10 kHz para 20 kHz ou vice versa
0005 32                 23                     RETI
                        24     ; ----------------------------------------------------------------------
                        25                     ORG     0Bh ; interrupção do timer 0
000B B2A0               26                     CPL     P2.0 ; complementa P2.0 (acende ou apaga o led)
                        27
                        28                     ; controle de fluxo
000D 300007             29                     JNB     20h.0, F20k ; testa o valor do bit de flag 20h.0
0010 758CFF             30                     MOV     TH0, #0FFh ; frequencia de 10 kHz
0013 758AD7             31                     MOV     TL0, #0D7h ; conta 41 microsegundospara no total dar 50
0016 32                 32                     RETI
                        33
0017 758CFF             34     F20K:           MOV     TH0, #0FFh ; frequencia de 20 kHz
001A 758AF1             35                     MOV     TL0, #0F1h ; conta 15 microsegundos para no total dar 25
001D 32                 36                     RETI
                        37     ; ----------------------------------------------------------------------
                        38
                        39     PROG:           ; programa principal
001E D2AF               40                     SETB    EA ; habilita interrupções
0020 D2A8               41                     SETB    EX0 ; seta interrupçao externa 0
0022 D2A9               42                     SETB    ET0 ; seta timer 0
0024 D288               43                     SETB    IT0 ; ex0 ativada com descida de borda
                        44
                        45                     ; Timer 1: tudo zero
                        46                     ; Timer 0:
                        47                             ; Gate = 0, controle por software
                        48                             ; T\ = 0, timer
                        49                             ; M1 = 0 e M0 = 1, modo 1 de temporizaçao
0026 758901             50                     MOV     TMOD, #01 ; 0000 0001
0029 D2A0               51                     SETB    P2.0 ; apaga o led
002B D200               52                     SETB    20h.0 ; bit de flag (10 kHz ou 20 kHz)
                        53
                        54     LOOP:           ; loop
002D 758CFF             55                     MOV     TH0, #0FFh ; frequencia de 10 kHz
0030 758AD7             56                     MOV     TL0, #0D7h ; conta 50 microsegundos
0033 D28C               57                     SETB    TR0 ; inicia a contagem
0035 2000FD             58                     JB      20h.0, $ ; espera a interrupçao quando o timer estourar
                        59                                      ;(cada 50 microsegundos)
0038 758CFF             60                     MOV     TH0, #0FFh ; frequencia de 20 kHz
003B 758AF1             61                     MOV     TL0, #0F1h
003E 3000FD             62                     JNB     20h.0,$ ; espera a interrupçao quando o timer estourar
0041 80EA               63                     SJMP    LOOP ; reinicia o loop
                        64
                        65                     END
ASSEMBLY COMPLETE, NO ERRORS FOUND, NO WARNINGS


