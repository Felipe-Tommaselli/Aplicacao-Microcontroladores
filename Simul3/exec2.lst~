0023 D288               22                     SETB    IT0 ; ex0 ativada com descida de borda
                        23
0025 75A0FF             24                     MOV     P2, #0FFh ; apaga todos os leds
                        25
                        26                     ; Timer 0: tudo zero
                        27                     ; Timer 1:
                        28                             ; Gate = 0, controle por software
                        29                             ; C = 1, contador
                        30                             ; M1 = 0 e M0 = 1, modo 1 de contagem
0028 758950             31                     MOV     TMOD, #50h
002B D28E               32                     SETB    TR1 ; habilita a contagem
002D 7400               33                     MOV     A, #0 ; flag variando entre 0, 1 e 2
                        34
                        35     LOOP:           ; loop
002F 758DFF             36                     MOV     TH1, #0FFh
0032 758BFB             37                     MOV     TL1, #0FBh ; carrega a contagem de 5 pulsos
0035 80FE               38                     SJMP    $ ; espera 5 pulsos na porta P3.2
                        39     ; ----------------------------------------------------------------------
                        40
                        41     CONT1:          ;
0037 B4000A             42                     CJNE    A, #00, T10
003A C2A0               43                     CLR     P2.0
003C 758DFF             44                     MOV     TH1, #0FFh
003F 758BFB             45                     MOV     TL1, #0FBh
0042 04                 46                     INC     A
0043 32                 47                     RETI
                        48     ; ----------------------------------------------------------------------
                        49
                        50     T10:            ; time 10
0044 B4010A             51                     CJNE    A, #01, T20
0047 C2A1               52                     CLR     P2.1
0049 758DFF             53                     MOV     TH1, #0FFh
004C 758BF6             54                     MOV     TL1, #0F6h
004F 04                 55                     INC     A
0050 32                 56                     RETI
                        57     ; ----------------------------------------------------------------------
                        58
                        59     T20:            ; time 20
